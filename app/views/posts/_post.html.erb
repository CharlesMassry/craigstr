<%= div_for(post) do %>
  <h1>
    <%= post.title %>
  </h1>
  <h3>
    <%= post.content %>
  </h3>
    <%= link_to "Edit Post", edit_region_category_post_path(post.region, post.category, post) %>
    <%= link_to "Delete Post", post_path(post), method: :delete, data: { confirm: "Are you sure?" } %>
</div>
<% end %>
